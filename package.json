{
  "name": "runp",
  "description": "Build software products using prompts.",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "dev": "next dev --turbo",
    "db:init": "pnpx prisma init",
    "db:generate": "pnpx prisma generate",
    "db:migrate-dev": "pnpx prisma migrate dev",
    "db:seed": "pnpx prisma db seed",
    "db:reset": "pnpx prisma migrate reset",
    "db:studio": "pnpx prisma studio",
    "inngest:dev": "pnpm dlx inngest-cli@latest dev",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^6.14.0",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.85.3",
    "@trpc/client": "^11.4.4",
    "@trpc/server": "^11.4.4",
    "@trpc/tanstack-react-query": "^11.4.4",
    "@usefui/analytics": "^1.0.2",
    "@usefui/components": "^1.5.0",
    "@usefui/core": "^1.3.2",
    "@usefui/hooks": "^1.3.2",
    "@usefui/icons": "^1.1.3",
    "@usefui/styles": "^1.4.2",
    "@usefui/tokens": "^1.5.1",
    "client-only": "^0.0.1",
    "inngest": "^3.40.1",
    "next": "^15.4.5",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "server-only": "^0.0.1",
    "sonner": "^1.7.4",
    "styled-components": "^6.1.19",
    "superjson": "^2.2.2",
    "zod": "^3.25.76",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/styled-components": "^5.1.34",
    "date-fns": "^4.1.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.5",
    "prettier": "^3.6.2",
    "prisma": "^6.14.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "pnpm@10.10.0"
}
