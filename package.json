{
  "name": "runp",
  "description": "Build software products using prompts.",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "dev": "next dev --turbo",
    "postinstall": "pnpx prisma generate",
    "db:init": "pnpx prisma init",
    "db:generate": "pnpx prisma generate",
    "db:migrate-dev": "pnpx prisma migrate dev",
    "db:seed": "pnpx prisma db seed",
    "db:reset": "pnpx prisma migrate reset",
    "db:studio": "pnpx prisma studio",
    "inngest:dev": "pnpm dlx inngest-cli@latest dev",
    "playground:build": "e2b template build --name runp-test-dev --cmd '/compile_page.sh'",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.32.0",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/language": "^6.11.3",
    "@e2b/code-interpreter": "^1.5.1",
    "@hookform/resolvers": "^3.10.0",
    "@inngest/agent-kit": "^0.9.0",
    "@lezer/highlight": "^1.2.1",
    "@paper-design/shaders-react": "^0.0.54",
    "@prisma/client": "^6.16.1",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.87.4",
    "@trpc/client": "^11.5.1",
    "@trpc/server": "^11.5.1",
    "@trpc/tanstack-react-query": "^11.5.1",
    "@uiw/codemirror-themes": "^4.25.1",
    "@uiw/react-codemirror": "^4.25.1",
    "@usefui/analytics": "^1.0.3",
    "@usefui/components": "^1.5.1",
    "@usefui/core": "^1.3.3",
    "@usefui/hooks": "^1.3.3",
    "@usefui/icons": "^1.1.4",
    "@usefui/styles": "^1.4.3",
    "@usefui/tokens": "^1.5.2",
    "client-only": "^0.0.1",
    "framer-motion": "^12.23.22",
    "import-in-the-middle": "^1.14.2",
    "inngest": "^3.40.3",
    "mermaid": "^11.12.0",
    "next": "^15.5.3",
    "openai": "^5.20.3",
    "rate-limiter-flexible": "^7.3.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "require-in-the-middle": "^7.5.2",
    "server-only": "^0.0.1",
    "sonner": "^1.7.4",
    "styled-components": "^6.1.19",
    "superjson": "^2.2.2",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^20.19.14",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/styled-components": "^5.1.34",
    "date-fns": "^4.1.0",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.3",
    "netlify-plugin-inngest": "^1.0.1",
    "prettier": "^3.6.2",
    "prisma": "^6.16.1",
    "random-word-slugs": "^0.1.7",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
